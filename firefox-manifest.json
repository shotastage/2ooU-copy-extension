{
    "manifest_version": 2,
    "name": "Lin9 Me URL Shortener",
    "version": "0.0.2",
    "description": "Shorten and share current web page URL",
    "homepage_url": "https://lin9.me",
    "default_locale": "ja",
    "browser_action": {
        "default_title": "Lin9 Shortener",
        "default_popup": "./build/popup.html",
        "default_icon": "icon.png"
    },
    "permissions": [
        "activeTab",
        "clipboardWrite"
    ]
}
